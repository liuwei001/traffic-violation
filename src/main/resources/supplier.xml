<?xml version="1.0" encoding="UTF-8"?>
<!-- 
违章查询接口供应商配置
url:城市供应商地址
city_code：菜单选择的城市编码
targetCode属性： 针对不同的供应商对城市编码的转换
 -->
<suppliers>
	
	<supplier>
		<description>默认供应商</description>
		<url>http://testapi.zonghengche.com:8807/weizhang/query</url>
	</supplier>
	
	<supplier>
		<description>广东--供应商</description>
		<url>http://apis.zonghengche.com:8807/weizhang/query</url>
		<cityCodes>
			<city_code targetCode="GD_GuangZhou">GD_GuangZhou</city_code>
			<city_code targetCode="GD_ShenZhen">GD_ShenZhen</city_code>
		</cityCodes>
	</supplier>
	
	<supplier>
		<description>北京--供应商</description>
		<url>http://apis.zonghengche.com:8807/weizhang/query</url>
		<cityCodes>
			<city_code targetCode="BJ">BJ</city_code>
			<city_code targetCode="BJ2">BJ2</city_code>
			<city_code targetCode="BJ3">BJ3</city_code>
		</cityCodes>
	</supplier>

</suppliers>