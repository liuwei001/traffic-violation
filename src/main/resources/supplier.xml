<suppliers>
	
	<supplier>
		<description>默认供应商</description>
		<url>http://testapi.zonghengche.com:8807/weizhang/query</url>
		<classname>com.traffic.weizhang.strategy.ZonghengcheSupplier</classname>
	</supplier>
	
	<supplier>
		<description>广东--供应商</description>
		<url>http://api.buding.cn/v3/violations</url>
		<classname>com.traffic.weizhang.strategy.BudingSupplier</classname>
		<cityCodes>
			<city_code targetCode="shenzhen">GD_ShenZhen</city_code>
		</cityCodes>
	</supplier>

</suppliers>