<?xml version="1.0" encoding="UTF-8"?>
<!-- 
违章查询接口供应商配置
url:城市供应商地址
city_code：菜单选择的城市编码
targetCode属性： 针对不同的供应商对城市编码的转换
 -->
<suppliers>
	
	<supplier>
		<name>车营科技</name>
		<url>http://apis.zonghengche.com:8807/weizhang/query</url>
		<classname>com.traffic.weizhang.strategy.ZonghengcheSupplier</classname>
	</supplier>
	
	<supplier>
		<name>北京步鼎方舟科技有限公司</name>
		<url>http://api.buding.cn/v3/violations</url>
		<classname>com.traffic.weizhang.strategy.BudingSupplier</classname>
		<cityCodes>
			<city_code targetCode="shenzhen">GD_ShenZhen</city_code>
			<city_code targetCode="guangzhou">GD_GuangZhou</city_code>
		</cityCodes>
	</supplier>

</suppliers>